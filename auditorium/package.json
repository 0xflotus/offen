{
  "name": "auditorium",
  "version": "0.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "budo index.js --live --pushstate",
    "test": "npm run server & (VAULT_HOST=http://localhost:9876 npm run mochify; status=$?; kill $(lsof -t -i:9876); exit $status)",
    "mochify": "mochify --chrome $(which google-chrome) --allow-chrome-as-root {,**}/*.test.js",
    "server": "http-server __fixtures__ -p 9876 -s",
    "pretest": "dependency-check package.json",
    "posttest": "standard",
    "fix": "standard --fix",
    "build": "gulp"
  },
  "dependencies": {
    "choo": "^6.13.3",
    "choo-devtools": "^2.5.1",
    "date-fns": "^1.30.1",
    "lodash": "^4.17.11",
    "offen": "file:./../packages",
    "plotly.js-basic-dist": "1.48.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "budo": "^11.6.2",
    "dependency-check": "^3.3.0",
    "envify": "^4.1.0",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.4.0",
    "gulp-rev": "^9.0.0",
    "gulp-rev-replace": "^0.4.4",
    "gulp-uglify": "^3.0.2",
    "html-inline": "^1.2.0",
    "http-server": "^0.11.1",
    "mochify": "^6.3.0",
    "run-sequence": "^2.2.1",
    "sheetify": "^7.4.0",
    "standard": "^12.0.1",
    "uglify-js": "^3.6.0",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "browserify": {
    "transform": [
      "envify",
      "sheetify",
      [
        "babelify",
        {
          "presets": [
            "@babel/preset-env"
          ]
        }
      ]
    ]
  },
  "standard": {
    "env": [
      "mocha"
    ]
  }
}
